    pass

def toString(p):
    p.sort(lambda a,b:b[0]-a[0])
    l = []
    for i in range(len(p)):
        if i > 0 and p[i][1] > 0:
            l.append("+")
        if p[i][1] == -1:
            if p[i][0] == 0:
                l.append("-1")
            else:
                l.append("-")
        elif p[i][1] == 1:
            if p[i][0] == 0:
                l.append("1")
        else:
            l.append(str(p[i][1]))
        if p[i][0] == 1:
            l.append("x")
        elif p[i][0] != 0:
            l.append("x^%d" % (p[i][0]))
    return "".join(l)

p1 = [[20,4],[10,3],[0,5]]
p2 = [[8,2],[1,1],[0,1]]
print(toString(mul(p1,p2)))
print(toString(div(p1,p2)))
